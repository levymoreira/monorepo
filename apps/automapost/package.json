{
  "name": "automapost",
  "version": "0.0.90",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:prod": "cp -f .env.production .env.local && next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "node server.js",
    "deploy": "cd ../.. && ./scripts/commit.sh && ./scripts/deploy.sh automapost",
    "grok": "node poc/grok.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop automapost",
    "pm2:restart": "pm2 restart automapost",
    "pm2:delete": "pm2 delete automapost",
    "pm2:logs": "pm2 logs automapost",
    "pm2:monit": "pm2 monit",
    "pm2:deploy": "yarn build && pm2 start ecosystem.config.js",
    "pm2:prod": "yarn build && (pm2 restart automapost || pm2 start ecosystem.config.js --env production)",
    "pm2:zero-downtime": "yarn build && (pm2 reload automapost --update-env || pm2 start ecosystem.config.js --env production)",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "db:introspect": "drizzle-kit introspect",
    "db:check": "drizzle-kit check",
    "test:performance": "playwright test --project=\"Mobile Chrome Performance\"",
    "test:performance:all": "playwright test",
    "test:performance:lcp": "playwright test tests/performance/lcp-mobile.spec.ts --project=\"Mobile Chrome Performance\"",
    "test:performance:inp": "playwright test tests/performance/inp-mobile.spec.ts --project=\"Mobile Chrome Performance\"",
    "test:performance:regression": "playwright test tests/performance/regression.spec.ts --project=\"Mobile Chrome Performance\"",
    "test:performance:headed": "playwright test --project=\"Mobile Chrome Performance\" --headed",
    "test:performance:debug": "playwright test --project=\"Mobile Chrome Performance\" --debug",
    "test:performance:production": "TEST_PRODUCTION=1 playwright test --project=\"Mobile Chrome Performance\"",
    "test:performance:setup": "playwright test tests/performance/setup-verification.spec.ts --project=\"Mobile Chrome Performance\"",
    "lighthouse:mobile": "lhci autorun --config=.lighthouserc.mobile.json",
    "lighthouse:all": "lhci autorun --config=.lighthouserc.json",
    "lighthouse:upload": "lhci upload --config=.lighthouserc.json",
    "performance:report": "yarn test:performance && yarn lighthouse:mobile",
    "performance:ci": "yarn test:performance && yarn lighthouse:mobile --assert",
    "kill": "pkill -f \"next\"",
    "delete:levy": "node tests/deleteLevyUser.js",
    "test:auth": "playwright test tests/auth/auth.spec.ts --project=\"Desktop Chrome Performance\"",
    "test:auth:headed": "playwright test tests/auth/auth.spec.ts --project=\"Desktop Chrome Performance\" --headed"
  },
