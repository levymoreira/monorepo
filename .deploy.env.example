# Deployment Configuration
# Copy this file to .deploy.env and fill in your values

# Azure Container Registry
ACR_REGISTRY=monoreporegistry.azurecr.io

# Server Configuration (for SSH deployment)
SERVER_HOST=your-server-ip-or-hostname
SERVER_USER=azureuser
SERVER_PATH=/opt/azure-sites-poc

# SSH Key (optional, defaults to ~/.ssh/id_rsa)
# SSH_KEY=~/.ssh/deploy_key.pem

# Project name (should match docker-compose.yml project name)
PROJECT_NAME=azure-sites-poc
